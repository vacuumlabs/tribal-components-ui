{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {},
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "isolatedModules": false,
    "jsx": "react-native",
    "lib": ["dom", "esnext", "es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "es2015",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
